{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.ts",
    "migrate": "cd database && npx sequelize-cli db:migrate",
    "migrate:create": "cd database && npx sequelize-cli migration:generate --name",
    "create:seed": "cd database && npx sequelize-cli seed:generate --name",
    "migrate:undo": "cd database && npx sequelize-cli db:migrate:undo",
    "seed:undo": "cd database && npx sequelize-cli db:seed:undo",
    "seed:all": "cd database && npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "sequelize": "^6.37.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.12.0",
    "@types/passport-jwt": "^4.0.1",
    "sequelize-cli": "^6.6.2"
  }
}
